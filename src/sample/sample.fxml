<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="252.0" maxWidth="400.0" minHeight="252.0" minWidth="400.0" prefHeight="252.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controller">
    <children>
        <Label layoutX="176.0" layoutY="24.0" text="Login">
            <font>
                <Font size="28.0"/>
            </font>
        </Label>
        <JFXTextField fx:id="tf_user" layoutX="144.0" layoutY="91.0" promptText="usename"/>
        <JFXPasswordField fx:id="pass_field" layoutX="143.0" layoutY="124.0" promptText="password"/>
        <JFXButton fx:id="btn_login" layoutX="166.0" layoutY="170.0" onAction="#pressLogin" prefHeight="32.0"
                   prefWidth="69.0" text="Login"/>
        <JFXButton fx:id="btn_reg" layoutX="107.0" layoutY="202.0" onAction="#pressRes"
                   text="Don't you have an account? Register"/>
    </children>
</AnchorPane>
